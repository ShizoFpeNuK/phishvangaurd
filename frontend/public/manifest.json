{
	"manifest_version": 3,
	"name": "PhishVanguard",
	"version": "1.0.0",
	"description": "This is a browser extension",
	"action": {
		"default_popup": "client/popup.html"
	},
	"background": {
		"service_worker": "client/background.js"
	},
	"web_accessible_resources": [
		{
			"resources": ["model/model.json"],
			"matches": ["<all_urls>"]
		}
	],
	"content_scripts": [],
	"permissions": ["storage", "tabs", "activeTab", "alarms"],
	"icons": {}
}
